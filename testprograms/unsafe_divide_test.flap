// Test divide operation in unsafe blocks

// Test register / register
result1 := unsafe int64 {
    rax <- 100
    rbx <- 5
    rax <- rax / rbx
} {
    x0 <- 100
    x1 <- 5
    x0 <- x0 / x1
} {
    a0 <- 100
    a1 <- 5
    a0 <- a0 / a1
}

printf("100 / 5 = %.0f\n", result1)

// Test register / immediate
result2 := unsafe int64 {
    rax <- 84
    rax <- rax / 7
} {
    x0 <- 84
    x0 <- x0 / 7
} {
    a0 <- 84
    a0 <- a0 / 7
}

printf("84 / 7 = %.0f\n", result2)
