// Minimal test: two outer loops, each with two inner loops
@ i in 0..<2  max 20 {
    @ j in 0..<2  max 20 {
        printf("a")
    }
    @ k in 0..<2  max 20 {
        printf("b")
    }
    printf("\n")
}

printf("Between\n")

@ i in 0..<2  max 20 {
    @ j in 0..<2  max 20 {
        printf("c")
    }
    @ k in 0..<2  max 20 {
        printf("d")
    }
    printf("\n")
}

printf("Done\n")
