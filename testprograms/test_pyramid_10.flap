// Pyramid with height 10 - exactly like ascii_art
height := 10

@ i in 0..<height  max 100000 {
    spaces := height - i - 1
    @ j in 0..<spaces  max 100000 {
        printf(" ")
    }

    stars := 2 * i + 1
    @ k in 0..<stars  max 100000 {
        printf("*")
    }

    printf("\n")
}
printf("Done\n")
