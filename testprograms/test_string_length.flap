// Test string length operator (#)
// Strings are maps, so # should return character count

// Empty string
s1 := ""
len1 := #s1
println(len1)

// Single character
s2 := "a"
len2 := #s2
println(len2)

// Short string
s3 := "hello"
len3 := #s3
println(len3)

// Longer string
s4 := "Hello, World!"
len4 := #s4
println(len4)

// String with spaces
s5 := "   "
len5 := #s5
println(len5)
