// Test match on numeric literals with guards
// Based on LANGUAGE.md syntax

testMatch := (n) => n {
    n == 1 -> "one"
    n == 2 -> "two"
    n == 3 -> "three"
    ~> "other"
}

printf("testMatch(1) = %s (expected: one)\n", testMatch(1))
printf("testMatch(2) = %s (expected: two)\n", testMatch(2))
printf("testMatch(3) = %s (expected: three)\n", testMatch(3))
printf("testMatch(5) = %s (expected: other)\n", testMatch(5))
