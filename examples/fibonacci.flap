// fibonacci.flap - Fibonacci sequence generator
// Demonstrates recursion and pattern matching
// Compile: flapc fibonacci.flap -o fibonacci

// Recursive Fibonacci
fib = n -> {
    | n <= 1 => n
    ~> fib(n - 1) + fib(n - 2)
}

// Generate Fibonacci sequence
println("Fibonacci sequence:")
println(fib(0))
println(fib(1))
println(fib(2))
println(fib(3))
println(fib(4))
println(fib(5))
println(fib(6))
println(fib(7))
println(fib(8))
println(fib(9))
println(fib(10))
