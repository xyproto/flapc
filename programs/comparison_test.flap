// comparison_test.flap - test all comparison operators
x := 10
y := 20
z := 10

// Test <
x < y {
    -> println("10 < 20: true")
    ~> println("10 < 20: false")
}

// Test >
y > x {
    -> println("20 > 10: true")
    ~> println("20 > 10: false")
}

// Test ==
x == z {
    -> println("10 == 10: true")
    ~> println("10 == 10: false")
}

// Test !=
x != y {
    -> println("10 != 20: true")
    ~> println("10 != 20: false")
}

// Test <=
x <= z {
    -> println("10 <= 10: true")
    ~> println("10 <= 10: false")
}

// Test >=
y >= x {
    -> println("20 >= 10: true")
    ~> println("20 >= 10: false")
}

exit(0)
