// nested_break_test.flap - test nested loops with ret and continue
printf("Nested loop with ret (exit inner loop):\n")

@ i in 0..<3 {
    printf("i=%.0f: ", i)
    @ j in 0..<5 {
        (j == 3) {
            -> ret @2
            ~> printf("%.0f ", j)
        }
    }
    printf("\n")
}

printf("\nNested loop with continue (continue inner loop):\n")
@ i in 0..<3 {
    printf("i=%.0f: ", i)
    @ j in 0..<5 {
        (j == 2) {
            -> @2
            ~> printf("%.0f ", j)
        }
    }
    printf("\n")
}

