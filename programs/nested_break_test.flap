// nested_break_test.flap - test nested loops with @0 and @N
printf("Nested loop with @0 (break inner loop):\n")

@1 i in range(3) {
    printf("i=%.0f: ", i)
    @2 j in range(5) {
        j == 3 {
            -> @0 ~> 0
        }
        printf("%.0f ", j)
    }
    printf("\n")
}

printf("\nNested loop with @2 (continue inner loop):\n")
@1 i in range(3) {
    printf("i=%.0f: ", i)
    @2 j in range(5) {
        j == 2 {
            -> @2 ~> 0
        }
        printf("%.0f ", j)
    }
    printf("\n")
}

exit(0)
