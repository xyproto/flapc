// nested_break_test.flap - test nested loops with break and continue
printf("Nested loop with break (break inner loop):\n")

@+ i in range(3) {
    printf("i=%.0f: ", i)
    @+ j in range(5) {
        (j == 3) {
            -> break
            ~> printf("%.0f ", j)
        }
    }
    printf("\n")
}

printf("\nNested loop with continue (continue inner loop):\n")
@+ i in range(3) {
    printf("i=%.0f: ", i)
    @+ j in range(5) {
        (j == 2) {
            -> continue
            ~> printf("%.0f ", j)
        }
    }
    printf("\n")
}

exit(0)
