// Compare both syntaxes
// Single param without parens (this works)
double1 := x => x * 2
result1 := double1(5.0)
printf("double1(5.0) = %ld\n", result1 as number)

// Single param WITH parens (does this work?)
double2 := (y) => y * 2
result2 := double2(5.0)
printf("double2(5.0) = %ld\n", result2 as number)
