// Test list_sum
list_sum = lst -> {
    total := 0.0
    @ item in lst {
        total <- total + item
    }
    total
}

nums = [1, 2, 3, 4, 5]
sum = list_sum(nums)
println("Sum of [1,2,3,4,5]:")
println(sum)

